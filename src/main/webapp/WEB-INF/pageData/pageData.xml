<?xml version="1.0" encoding="UTF-8"?>
<root>
	<!--
		path 标签可循环嵌套，并且父级与子级的 class 同步有效，即 /a/..、/a/b/.. 同时有效。

		@name	必选		目录名称。
		@class	可选		解析的类，指定的类在实际访问时并不一定是该类，实例会根据版本或定制自动路由。


		使用示例：
		<path name="a.html" class="com.A" />

		<path name="b">
			<path name="c.html" class="com.C" />
		</path>

		<path name="d" class="com.D">
			<path name="e.html" class="com.E" />
		</path>
	-->


	<!-- 全站都要添加的 -->
	<path name="/" class="com.yy.web.pagedata.SiteData" />
	
	
	<!-- 登录页需要的操作。 -->
	<path name="login.html" class="com.yy.web.pagedata.LoginPageData" />
	<path name="login-dialog.html" class="com.yy.web.pagedata.LoginPageData" />
	

	<!-- 百图网。 -->
	<path name="tu">
		<!-- PC 端页面。 -->
		<path name="pc">
			<path name="index.html" class="com.yy.web.pagedata.tu.pic.IndexPageData" />
			<path name="list.html" class="com.yy.web.pagedata.tu.pic.ListPageData" />
			<path name="detail.html" class="com.yy.web.pagedata.tu.pic.DetailPageData" />

			<path name="traditionalList.html" class="com.yy.web.pagedata.tu.pic.ListPageData" />
		</path>
		<path name="pc1">
			<path name="index.html" class="com.yy.web.pagedata.tu.pic.IndexPageData" />
			<path name="list.html" class="com.yy.web.pagedata.tu.pic.ListPageData" />
			<path name="detail.html" class="com.yy.web.pagedata.tu.pic.DetailPageData" />
		</path>

		<path name="wap" class="com.yy.web.pagedata.tu.wap.PathData">
			<path name="detail.html" class="com.yy.web.pagedata.tu.pic.DetailPageData" />
		</path>

		<path name="manage" class="com.yy.web.pagedata.tu.pic.manage.ManagePathData" />
	</path>
	
	
	<!-- 收藏夹。 -->
	<path name="fav" class="com.yy.web.pagedata.fav.FavSiteData" />


	<!-- 站点建设。 -->
	<path name="xiaozhan">
		<path name="site" class="com.yy.web.pagedata.xiaozhan.site.SitePathData">
			<path name="*">
				<path name="index.html" class="com.yy.web.pagedata.xiaozhan.site.IndexPageData" />
				<path name="about.html" class="com.yy.web.pagedata.xiaozhan.site.AboutPageData" />
				<path name="contact.html" class="com.yy.web.pagedata.xiaozhan.site.ContactPageData" />
				
				<path name="product.html" class="com.yy.web.pagedata.xiaozhan.site.ProductPageData" />
				<path name="product-detail.html" class="com.yy.web.pagedata.xiaozhan.site.ProductDetailPageData" />
				
				<path name="article.html" class="com.yy.web.pagedata.xiaozhan.site.ArticlePageData" />
				<path name="article-detail.html" class="com.yy.web.pagedata.xiaozhan.site.ArticleDetailPageData" />
				
				<path name="case.html" class="com.yy.web.pagedata.xiaozhan.site.CasePageData" />
				<path name="case-detail.html" class="com.yy.web.pagedata.xiaozhan.site.CaseDetailPageData" />
				
				<path name="search.html" class="com.yy.web.pagedata.xiaozhan.site.SearchPageData" />
			</path>
		</path>

		<path name="manage" class="com.yy.web.pagedata.xiaozhan.manage.ManagePathData" />
	</path>
	
	
	<!-- 文件上传。 -->
	<path name="file" class="com.yy.web.pagedata.file.PathData">
		<path name="index.html" class="com.yy.web.pagedata.file.IndexPageData" />
		<path name="list.html" class="com.yy.web.pagedata.file.ListPageData" />
		<path name="detail.html" class="com.yy.web.pagedata.file.DetailPageData" />
	</path>
</root>
